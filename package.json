{
  "name": "tw-stock-app",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "fetch": "node scripts/fetch-yahoo.mjs",
    "fetch:list": "node scripts/fetch-stock-list.mjs",
    "fetch:revenue": "node scripts/fetch-revenue.mjs",
    "fetch:stats": "node scripts/fetch-monthly-stats.mjs",
    "fetch:financials": "node scripts/fetch-financials.mjs",
    "fetch:chips": "node scripts/fetch-chips.mjs",
    "fetch:all": "npm run fetch:list && npm run fetch:revenue && npm run fetch:stats && npm run fetch:financials && npm run fetch:chips",
    "build:db": "node scripts/build-sqlite-db.js",
    "refresh-db": "npm run fetch:all && npm run build:db"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/node": "^9.5.2",
    "astro": "^5.16.15",
    "chartgpu": "^0.1.6",
    "sql.js": "^1.13.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "better-sqlite3": "^12.6.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "happy-dom": "^20.3.7",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "vitest": "^4.0.18"
  }
}